{
  "name": "@favware/dexa",
  "version": "2.1.0",
  "description": "Amazing pokedex skill for Alexa",
  "author": "@favware",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://favware.tech/dexa",
  "private": true,
  "scripts": {
    "lint": "eslint src scripts __tests__ --ext ts --fix",
    "style": "yarn pretty-quick",
    "test": "jest",
    "build": "tsc -b src",
    "watch": "tsc -b src -w",
    "up": "yarn upgrade-interactive --latest",
    "predeploy": "rm -rf dist && yarn build",
    "deploy": "run-s deploy:prep deploy:install-packages deploy:zip deploy:push",
    "deploy:prep": "yarn scripts:prepareawsfunction",
    "deploy:install-packages": "cd dist && yarn --ignore-scripts --no-lockfile --production --ignore-optional --non-interactive --no-bin-links --link-duplicates",
    "deploy:zip": "cd dist && zip -rq function.zip .",
    "deploy:push": "cd dist && aws lambda update-function-code --function-name alexa_dexa --zip-file fileb://function.zip",
    "sversion": "standard-version",
    "scripts:runner": "ts-node -P tsconfig.eslint.json",
    "scripts:skilldatagenerator": "yarn scripts:runner scripts/skillDataGenerator",
    "scripts:prepareawsfunction": "yarn scripts:runner scripts/prepareAwsFunction"
  },
  "dependencies": {
    "@klasa/utils": "^0.0.8",
    "alexa-app": "^4.2.3",
    "apollo-boost": "^0.4.9",
    "confusables": "^1.0.0",
    "cross-fetch": "^3.0.5",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0"
  },
  "devDependencies": {
    "@favware/eslint-config": "5.1.0",
    "@favware/graphql-pokemon": "^4.2.4",
    "@favware/ts-config": "^3.0.3",
    "@playlyfe/gql": "^2.6.2",
    "@types/common-tags": "^1.8.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.10",
    "common-tags": "^1.8.0",
    "eslint": "^7.6.0",
    "express": "^4.17.1",
    "fs-nextra": "^0.5.1",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "jest-circus": "^26.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/dexa.git"
  },
  "bugs": {
    "url": "https://github.com/favware/dexa/issues"
  },
  "keywords": [
    "favware",
    "typescript",
    "ts",
    "yarn",
    "alexa",
    "alexa-app",
    "pokemon",
    "dexa"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "resolutions": {
    "acorn": "^7.2.0",
    "minimist": "^1.2.5",
    "kind-of": "^6.0.3"
  }
}
