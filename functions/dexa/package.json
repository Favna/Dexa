{
  "name": "dexa",
  "version": "1.2.0",
  "description": "Simple pokedex skill for Alexa",
  "main": "functions/dexa/index.js",
  "repository": "git@github.com:Favna/Dexa.git",
  "author": "Jeroen Claassens <jeroen.claassens@live.nl>",
  "license": "GPL-3.0",
  "private": false,
  "dependencies": {
    "alexa-app": "^4.2.2",
    "common-tags": "^1.8.0",
    "fuse.js": "^3.2.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.4.0",
    "express": "^4.16.3",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0"
  },
  "scripts": {
    "deploy": "cd ../../ && apex deploy && cd functions/dexa",
    "docs": "jsdoc2md --template ../../docs/template.hbs --files intents/*.js | tee ../../docs/docs.md > NUL",
    "docs:json": "jsdoc2md --json --files intents/*.js > ../../../favna.xyz/public/dist/dexadocs.json",
    "lint": "eslint **/*.js",
    "lint:fix": "eslint --fix **/*.js",
    "skill": "node skill.js",
    "test": "yarn lint && mocha test"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "directories": {
    "doc": "./docs",
    "lib": "./functions/dexa"
  }
}
