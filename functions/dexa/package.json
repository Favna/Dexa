{
  "name": "dexa",
  "version": "1.2.1",
  "description": "Simple pokedex skill for Alexa",
  "main": "functions/dexa/index.js",
  "repository": "git@github.com:Favna/Dexa.git",
  "author": "Jeroen Claassens <jeroen.claassens@live.nl>",
  "license": "GPL-3.0",
  "private": false,
  "dependencies": {
    "alexa-app": "^4.2.3",
    "common-tags": "^1.8.0",
    "fuse.js": "^3.4.5",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "express": "^4.17.1",
    "jsdoc-to-markdown": "^5.0.0",
    "mocha": "^6.1.4",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "deploy": "pushd ../../ && apex deploy && popd",
    "docs": "node_modules/.bin/jsdoc2md --template ../../docs/template.hbs --files intents/*.js | tee ../../docs/docs.md > NUL",
    "docs:json": "node_modules/.bin/jsdoc2md --json --files intents/*.js > ../../../@favware-website/src/assets/docs/dexa.json",
    "lint": "node_modules/.bin/eslint --fix intents/*.js *.js",
    "skill": "node skill.js",
    "test": "mocha test"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "directories": {
    "doc": "./docs",
    "lib": "./functions/dexa"
  }
}
